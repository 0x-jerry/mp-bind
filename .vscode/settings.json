{
  "json.schemas": [
    {
      "fileMatch": ["/src/app.json"],
      "url": "https://raw.githubusercontent.com/qiu8310/minapp/master/schema/app.json"
    },
    {
      "fileMatch": ["/src/pages/**.json"],
      "url": "https://raw.githubusercontent.com/qiu8310/minapp/master/schema/page.json"
    },
    {
      "fileMatch": ["/src/components/**.json"],
      "url": "https://raw.githubusercontent.com/qiu8310/minapp/master/schema/component.json"
    },
    {
      "fileMatch": ["/project.config.json"],
      "url": ".vscode/project.config.schema.json"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "miniprogram/**/*.js": true
  }
}
