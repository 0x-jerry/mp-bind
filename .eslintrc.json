{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "standard",
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "wx": true,
    "App": true,
    "Page": true,
    "Component": true,
    "getApp": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
